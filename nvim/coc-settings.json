{
  "browser.maxLength": 30,
  "browser.priority": 0,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [],
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.messageLevel": "more",
  "coc.source.around.priority": 8,
  "coc.source.buffer.priority": 1,
  "coc.source.dictionary.priority": 1,
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "coc.source.emoji.priority": 3,
  "coc.source.emoji.shortcut": "E",
  "coc.source.file.priority": 10,
  "coc.source.tag.priority": 5,
  "coc.source.vimtex.priority": 99,
  "diagnostic-languageserver.filetypes": {
    "diagnostic.hintSign": "➤",
    "diagnostic.infoSign": "ℹ"
  },
  "emmet.includeLanguages": { "vue": "html", "vue-html": "html" },
  "eslint.autoFix": false,
  "eslint.autoFixOnSave": false,
  "eslint.enable": true,
  "eslint.filetypes": ["javascript", "javascript.jsx", "javascriptreact"],
  "explorer.file.column.git.showIgnored": true,
  "explorer.file.column.indent.indentLine": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings": {
    "k": "nodePrev",
    "j": "nodeNext",
    "*": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "l": ["expandable?", "expand", "open"],
    "J": ["toggleSelection", "normal:j"],
    "K": ["toggleSelection", "normal:k"],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "o": "expandOrCollapse",
    "<cr>": "open",
    "e": "open",
    "E": "openInVsplit",
    "t": "openInTab",
    "<bs>": "gotoParent",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "X": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "gk": "gitPrev",
    "gj": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "explorer.openAction.changeDirectory": true,
  "explorer.file.tabCD": false,
  "explorer.previewAction.onHover": false,
  "explorer.quitOnOpen": false,
  "explorer.width": 30,
  "git.addedSign.hlGroup": "GitAdd",
  "git.changeRemovedSign.hlGroup": "GitDeleteTopAndBottom",
  "git.changedSign.hlGroup": "GitModify",
  "git.enableGutters": true,
  "git.removedSign.hlGroup": "GitDeleteBottom",
  "git.topRemovedSign.hlGroup": "GitDeleteTop",
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "format": "b",
          "directory": "/home/voldikss/.cache/nvim/ccls-cache"
        },
        "capabilities": {
          "foldingRangeProvider": false,
          "workspace": { "workspaceFolders": { "supported": false } }
        },
        "clang": {
          "extraArgs": ["--gcc-toolchain=/usr"],
          "resourceDir": "/usr/lib/clang/9.0.1"
        },
        "completion": { "detailedLabel": true },
        "index": { "onChange": false, "initialNoLinkage": true, "threads": 2 }
      }
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh", "zsh"],
      "ignoredRootPaths": ["~"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim", ".git"],
      "filetypes": ["go"]
    },
    "lua": { "command": "lua-lsp", "filetypes": ["lua"] }
  },
  "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
  "marketplace.npmsio": true,
  "prettier.disableLanguages": ["typescript"],
  "prettier.formatterPriority": 1,
  "prettier.printWidth": 85,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "python.autoComplete.addBrackets": true,
  "python.formatting.provider": "black",
  "session.directory": "~/.cache/nvim/session",
  "session.saveOnVimLeave": false,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "snippets.priority": -10,
  "suggest.completionItemKindLabels": {
    "function": "\u0192",
    "method": "\uf09a",
    "variable": "\ue79b",
    "constant": "\uf8fe",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "file": "\uf723",
    "value": "\uf89f",
    "event": "\ufacd",
    "folder": "\uf114",
    "keyword": "\uf1de",
    "snippet": "\ue60b",
    "operator": "\u03a8",
    "reference": "\ufa46",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "suggest.detailMaxLength": 50,
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": false,
  "suggest.labelMaxLength": 50,
  "suggest.languageSourcePriority": 99,
  "suggest.noselect": false,
  "suggest.numberSelect": false,
  "suggest.removeDuplicateItems": true,
  "suggest.triggerAfterInsertEnter": false,
  "template.templatesDirectory": "~/dotfiles/nvim/templates",
  "todolist.autoUpload": false,
  "todolist.monitor": true,
  "translator.engines": ["bing", "ciba", "google"],
  "tslint.autoFixOnSave": true,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.suggestionActions.enabled": false,
  "yank.enableCompletion": false,
  "yank.highlight.duration": 20,
  "yank.highlight.enable": true,
  "yank.priority": 4
  // vim:ft=jsonc
}
