  "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
  "list.indicator": "⚡️",
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-j>": "do:next",
    "<C-k>": "do:previous",
    "<C-r>": "action:preview",
    "<C-t>": "do:toggle"
  },
  "list.normalMappings": {
    "<C-c>": "do:exit",
    "<C-j>": "do:next",
    "<C-k>": "do:previous",
    "<C-p>": "action:preview",
    "p": "action:preview",
    "<C-t>": "do:toggle"
  },
  "list.previewSplitRight": false,
  "python.autoComplete.addBrackets": true,
  "python.formatting.provider": "black",
  "session.directory": "~/.cache/nvim/session",
  "session.saveOnVimLeave": false,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "snippets.priority": -10,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf9be",
    "file": "\uf723",
    "folder": "\uf114",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf09a",
    "module": "\uf40d",
    "operator": "\u03a8",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue79b"
  },
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.detailMaxLength": 50,
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": false,
  "suggest.labelMaxLength": 75,
  "suggest.languageSourcePriority": 99,
  "suggest.noselect": false,
  "suggest.numberSelect": false,
  "suggest.removeDuplicateItems": false,
  "suggest.triggerAfterInsertEnter": false,
  // vim:ft=jsonc
}
