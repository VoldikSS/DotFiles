{
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.formatOnSaveFiletypes": [],
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.messageLevel": "more",
  "coc.preferences.promptWorkspaceEdit": true,
  "coc.preferences.rootPatterns": [
    ".git",
    "Cargo.toml",
    ".root",
    ".gitignore",
    "Makefile",
    ".tasks",
    "CMakeLists.txt"
  ],
  "coc.source.around.priority": 8,
  "coc.source.buffer.priority": 1,
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "coc.source.emoji.priority": 3,
  "coc.source.emoji.shortcut": "E",
  "coc.source.file.priority": 10,
  "coc.source.tag.priority": 5,
  "cursors.cancelKey": "<esc>",
  "diagnostic-languageserver.filetypes": {
    "diagnostic.hintSign": "➤",
    "diagnostic.infoSign": "ℹ"
  },
  "explorer.file.column.git.showIgnored": true,
  "explorer.file.column.indent.indentLine": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings": {
    "k": "nodePrev",
    "<C-k>": "nodePrev",
    "<M-k>": "indentPrev",
    "j": "nodeNext",
    "<C-j>": "nodeNext",
    "<M-j>": "indentNext",
    "*": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "l": ["expandable?", "expand", "open"],
    "J": ["toggleSelection", "normal:j"],
    "K": ["toggleSelection", "normal:k"],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "o": "expandOrCollapse",
    "<cr>": "open",
    "<C-l>": "open",
    "e": "open",
    "E": "openInVsplit",
    "t": "openInTab",
    "<bs>": "gotoParent",
    "<C-h>": "gotoParent",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "P": "preview",
    "d": "delete",
    "D": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "X": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "ga": "gitStage",
    "gu": "gitUnstage",
    "gk": "gitPrev",
    "gj": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "explorer.file.tabCD": false,
  "explorer.previewAction.onHover": false,
  "explorer.quitOnOpen": false,
  "explorer.width": 30,
  "git.addedSign.hlGroup": "GitAdd",
  "git.changeRemovedSign.hlGroup": "GitDeleteTopAndBottom",
  "git.changedSign.hlGroup": "GitModify",
  "git.enableGutters": true,
  "git.removedSign.hlGroup": "GitDeleteBottom",
  "git.semanticCommit.scope": false,
  "git.showCommitInFloating": true,
  "git.topRemovedSign.hlGroup": "GitDeleteTop",
